{
  "PostgreSql": {
    "ConnectionString": "Host=localhost;Username=postgres;Database=myDatabase",
    "DbPassword": "mysecretpassword"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "WithClientIp",
      "WithClientAgent"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:321"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "OidcSettings": {
    "Authority": "https://localhost:5001",
    "RequiredScope": [
      "KalanalyzeCode.ConfigurationManager"
    ],
  }
}
