<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            <MudIcon Icon="@Icons.Material.Filled.DeleteForever" Class="mr-3 mb-n1"/>
            Delete project?
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField Value="@Model?.Id.ToString()" Label="Project ID" ReadOnly="true"/>
        <MudTextField Value="@(Model?.Name)"
                      Label="Project Name" ReadOnly="true"/>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="@(Cancel)">Cancel</MudButton>
        <MudButton Color="Color.Error" OnClick="@(async () => await DeleteMotor(CancellationToken))">Delete Ship</MudButton>
    </DialogActions>
</MudDialog>